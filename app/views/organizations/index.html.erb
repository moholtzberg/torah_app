<h1>Organizations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organizations.each do |organization| %>
      <tr>
        <td>Name:<%= organization.name %></td>
      </tr>
      <tr>
        <td>Founder:<%= organization.founder.name %> </td>
        <td> <%= link_to 'Show', organization %></td>
      </tr>
    <% end %>
  </tbody>
</table>
